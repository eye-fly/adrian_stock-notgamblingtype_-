                        <div id="subcategories">
                            <h2 class="subcategory-heading">Podkategorie</h2>
                            <div class="subcat-slider owl-carousel owl-theme owl-loaded">
                                            
                                            
                                            
                                            
                                            
                                        <div class="owl-stage-outer" style="padding-left: 0px; padding-right: 0px;"><div class="owl-stage" style="width: 765.625px; transition-duration: 0s; transform: translate3d(0px, 0px, 0px);"><div class="owl-item active" style="width: 136.125px; margin-right: 17px;"><div>
